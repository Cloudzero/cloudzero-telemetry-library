# CloudZero Telemetry Environment Variables
# Copy this file to .env and fill in your values

# CloudZero API Configuration
CLOUDZERO_API_KEY=your-cloudzero-api-key-here

# Snowflake Configuration
SNOWFLAKE_USER=your-snowflake-username
SNOWFLAKE_PASSWORD=your-snowflake-password
SNOWFLAKE_ACCOUNT=your-snowflake-account-identifier

# Secrets Provider (env, aws, azure, gcp)
SECRETS_PROVIDER=env

# Optional: Advanced Configuration
TELEMETRY_URL=https://api.cloudzero.com/unit-cost/v1/telemetry
MAX_RECORDS_PER_CALL=3000
DEFAULT_WAREHOUSE=COMPUTE_WH
DATA_LATENCY_HOURS=1
LOG_LEVEL=INFO